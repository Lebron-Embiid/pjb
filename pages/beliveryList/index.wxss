/* pages/buildList/index.wxss */
.build_box{
  padding-bottom: 150rpx;
  box-sizing: border-box;
}
.house_item{
  padding: 30rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.house_item image{
  width: 100rpx;
  height: 100rpx;
  background-color: #cccccc;
  border-radius: 10rpx;
  margin-right: 20rpx;
}
.house_box{
  width: 70%;
  font-size: 22rpx;
}
.house_edit{
  width: 30%;
  text-align: right;
}
.house_edit button{
  font-size: 24rpx;
  margin-bottom: 5rpx;
}