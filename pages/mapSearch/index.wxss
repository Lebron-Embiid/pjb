.section{
  height: 45px;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.section input{
  width:100%;
  /* position: absolute; */
  /* left: 48rpx; */
  /* top: 18rpx; */
  border:1px solid #c3c3c3;
  /* height:25px; */
  border-radius: 3px;
  padding: 0 10rpx;
  font-size: 24rpx;
}
.text_box{
  margin: 10px 20px;
  border-bottom: 1px solid #c3c3c3;
  padding-bottom: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}